    "@testing-library/user-event": "^13.1.9",
    "axios": "^0.21.1",
    "cypress-axe": "^0.12.2",
    "faker": "^5.5.3",
    "firebase": "^9.0.2",
    "firebase-mock": "^2.3.2",
    "firebase-tools": "^9.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.1",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^4.2.0",
    "react-query": "^3.8.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.3",
    "resolve": "^1.19.0",
    "scheduler": "^0.20.1",
    "workbox-build": "^6.1.0",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-recipes": "^6.1.0",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@emotion/eslint-plugin": "^11.0.0",
    "@firebase/firestore-types": "^2.1.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/node": "15.12.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/scheduler": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "axe-core": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "7.5.0",
    "dotenv": "10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "6.0.0",
    "is-ci": "^2.0.0",
    "jest": "26.6.0",
    "lint-staged": "11.0.0",
    "msw": "^0.26.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "cross-env PORT=8888 react-scripts start",
    "dev": "react-scripts start",
    "serve": "serve -s build --no-clipboard",
    "db": "firebase emulators:start --import=./dev-data --export-on-exit",
    "setup": "npm install && npm run validate",
    "verify": "run-s build test",
    "tsTypeVerify": "tsc --watch",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|yml|yaml|html)\"",
    "lint": "eslint --fix --ext js,jsx,ts,tsx --ignore-path .eslintignore .",
    "lint:ci": "eslint --ext js,jsx,ts,tsx --ignore-path .eslintignore .",
    "test": "jest",
    "test:precommit": "jest --watch=false --findRelatedTests",
    "test:coverage": "jest --watch=false --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test start 8888 cy:run",
    "test:e2e:ci": "start-server-and-test serve 5000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "npx cypress verify",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "prepare"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "npm run test:precommit",
      "pretty-quick --staged"
    ],
    "*.+(json|yml|yaml|css|less|scss)": [
      "pretty-quick --staged"
    ]
  }
}
