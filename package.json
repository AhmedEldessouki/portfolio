{
  "name": "portfolio",
  "version": "2.0.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.4",
    "cypress-axe": "^0.13.0",
    "faker": "^5.5.3",
    "firebase": "^9.0.2",
    "firebase-mock": "^2.3.2",
    "firebase-tools": "^9.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.0",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^4.2.0",
    "react-query": "^3.23.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^8.0.2",
    "resolve": "^1.19.0",
    "scheduler": "^0.20.1",
    "workbox-build": "^6.1.0",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-recipes": "^6.1.0",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@emotion/eslint-plugin": "^11.0.0",
    "@firebase/firestore-types": "^2.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/faker": "^5.5.8",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.9",
    "@types/scheduler": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "axe-core": "^4.3.3",
    "cross-env": "^7.0.3",
    "cypress": "^8.4.0",
    "dotenv": "10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.1",
    "serve": "^12.0.1",
    "start-server-and-test": "^1.14.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "cross-env PORT=8888 react-scripts start",
    "dev": "react-scripts start",
    "serve": "serve -s build --no-clipboard",
    "db": "firebase emulators:start --import=./dev-data --export-on-exit",
    "setup": "yarn && yarn validate",
    "verify": "run-s build test",
    "tsTypeVerify": "tsc --watch",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|yml|yaml|html)\"",
    "lint": "eslint --fix --ext js,jsx,ts,tsx --ignore-path .eslintignore .",
    "lint:ci": "eslint --ext js,jsx,ts,tsx --ignore-path .eslintignore .",
    "test": "react-scripts test",
    "test:precommit": "yarn test --watch=false --findRelatedTests",
    "test:coverage": "yarn test --watch=false --coverage",
    "test:watch": "yarn test --watch",
    "test:e2e:n": "start-server-and-test start 8888 cy:run",
    "test:e2e:ci": "start-server-and-test serve 5000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "npx cypress verify",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "prepare"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "npm run test:precommit",
      "pretty-quick --staged"
    ],
    "*.+(json|yml|yaml|css|less|scss)": [
      "pretty-quick --staged"
    ]
  }
}
